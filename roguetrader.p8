pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
#include constants.lua
#include lib.lua

create('circle',{x=64,y=64,w=80,h=9,c_f=8,c_b=9,text="abc",t_align="center"})
function _update()
	if btn(0) then gs.circle.x-=1 end
	if btn(1) then gs.circle.x+=1 end
	if btn(2) then gs.circle.y-=1 end
	if btn(3) then gs.circle.y+=1 end
  if btnp(4) then gs.circle.active = not gs.circle.active end
  if btnp(5) then gs.circle:execute() end
end

function _draw()
  cls()
  gs.circle:draw()
end
